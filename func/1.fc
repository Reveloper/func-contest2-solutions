}

(int) gcd(int a, int b) method_id {

    if ((a == 0) | (b == 0))
            {
            return max(a, b);
            }
    else {
        if(a > b)
                {
                    return gcd(a - b, b);
                }
        else
                {
                    return gcd(a, b - a);
                }
    }
}



